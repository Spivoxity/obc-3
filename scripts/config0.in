# Magic config
ROOT=@ROOT@
TOPDIR=@ABS_SRCDIR@
BINDIR=@ABS_BINDIR@
VERSION=@PACKAGE_VERSION@
REVID=unspecified
EXEEXT=@EXEEXT@
ENABLE_JIT=@ENABLE_JIT@
MACOS=@MACOS@
CC="@CC@"
obc1=$BINDIR/compiler/obc1
oblink=$BINDIR/runtime/oblink
obx=$BINDIR/obx
obxj=$BINDIR/obxj
obb1=$BINDIR/compiler/obb1
obdb1=@OBDB1_DEBUG@
obxdeb=$BINDIR/obxdeb
stdlib="-I $ROOT$BINDIR/lib -I $ROOT$TOPDIR/lib"
stdinc="-I $ROOT$BINDIR -I $ROOT$TOPDIR/runtime -I $ROOT$BINDIR/runtime -I $ROOT$BINDIR/lib"
rundir=$BINDIR/runtime
thundir=$BINDIR/thunder
libdir=$BINDIR/lib
DLFLAGS=@DLFLAGS@
LIBS="@LIBS@"
# End of config
